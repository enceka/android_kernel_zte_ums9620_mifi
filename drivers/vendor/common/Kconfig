config VENDOR_SOC_SPRD_COMPILE
	bool "drivers/vendor/soc/sprd drivers compile control"
	default n
	help
	  This enables drivers/vendor/soc/sprd drivers to be compiled.

config SOC_QCOM
	bool "qcom common drivers"
	default n
	help
	  This enables support for common driver in qcom platform.

config SOC_MTK
	bool "MTK common drivers"
	default n
	help
	  This enables support for common driver in MTK platform.

config SOC_SPRD
	bool "spred common drivers"
	default n
	help
	  This enables support for common driver in sprd platform.

config VENDOR_SQC_CHARGER
	bool "sqc charger platform common drivers"
	default n
	help
	  This enables support for platform sqc common driver.

